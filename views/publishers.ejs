<div class="publishers-container">
  <h2>List of Publishers</h2>
  <div class="publishers-gallery">
      <% rows.forEach(publisher => { %>
          <div class="publisher-item">
              <a href="/publishers/<%= publisher.id %>">
                  <img src="<%= publisher.image %>" alt="<%= publisher.name %>" class="publisher-image">
                  <p class="publisher-name"><%= publisher.name %></p>
              </a>
          </div>
      <% }); %>
  </div>

  <a href="/publishers/form?action=add">Add New Publisher</a>
</div>
